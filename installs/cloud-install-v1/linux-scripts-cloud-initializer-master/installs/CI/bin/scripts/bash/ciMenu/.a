alias a='. $ciEnv/.a'
alias e='. $ciEnv/.e'
alias m='. $ciEnv/.m'
alias cdci='cd /opt/CI'
alias cdbin='cdci; cd ./bin'
alias cdbootstraps='cdci; cd ./bin/scripts/bash/bootstraps'
alias cdapps='cdbootstraps; cd ./apps'
alias cdscripts='cdbin; cd scripts'
alias cdreinstall='cdbootstraps; cd reinstalls'
alias cduninstall='cdbootstraps; cd uninstalls'
alias cdutils='cdscripts; cd ./ciUtils'
alias cdhtml='cd /var/www/html'
alias ci='cloudinit'
alias ciclean='clear; ci clean'
alias cidelete='clear; ci delete'
alias cistatus='clear; ci status'
alias cistart='clear; ci start'
alias cirestart='clear; ci restart'
alias restartall='ci restart all'
alias cistop='clear; ci stop'
alias cireinstall='/opt/CI/bin/scripts/bash/bootstraps/reinstalls/reinstallApp.sh'
alias ciuninstall='/opt/CI/bin/scripts/bash/bootstraps/uninstalls/uninstallApp.sh'
alias va='vi $ciEnv/.a;a'
alias ve='vi $ciEnv/.e;e'
alias vm='vi $ciEnv/.m;m'
